    "import cv2\n",
    "im = cv2.imread('seg_bg.png')\n",
    "print im.shape\n",
    "\n",
    "for i in range(0,3917):\n",
    "    for j in range(0,4143):\n",
    "        for k in range(0,3):\n",
    "            if im[i,j,k] > 50:\n",
    "                im[i,j,k] = 255\n",
    "            else:\n",
    "                im[i,j,k] = 0\n",
    "cv2.imwrite('seg_bg_mask.png', im)\n"
   ]
